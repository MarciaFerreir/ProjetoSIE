    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.fatura && $data.fatura_cliente && $data.fatura_validacao ){",
        "\t//attach lazy references variables to parent variables",
        "\t$data.fatura.cliente = $data.fatura_cliente;",
        "\t$data.fatura.validacao = $data.fatura_validacao;",
        "\treturn {",
        "\t\t//map fatura variable to expected task contract input",
        "\t\tfaturaInput: {",
        "\t\t\tvalidacao: $data.fatura.validacao ? {",
        "\t\t\t\tpersistenceId_string: $data.fatura.validacao.persistenceId_string !== undefined ? $data.fatura.validacao.persistenceId_string : null,",
