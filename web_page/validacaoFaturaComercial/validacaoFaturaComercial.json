      "type" : "expression",
      "value" : [
        "if($data.formOutput && $data.formOutput._submitError && $data.formOutput._submitError.explanations){",
        "\tconst liElements = $data.formOutput._submitError.explanations",
        "\t\t.filter(cause => cause !== null)",
        "\t\t.map(cause => \"<li>\" + cause + \"</li>\")",
        "\t\t.join('');",
        "\tif(liElements){",
        "\t\treturn \"<ul>\" + liElements + \"</ul>\";",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.fatura && $data.fatura_cliente && $data.fatura_validacao ){",
        "\t//attach lazy references variables to parent variables",
        "\t$data.fatura.cliente = $data.fatura_cliente;",
        "\t$data.fatura.validacao = $data.fatura_validacao;",
        "\treturn {",
        "\t\t//map fatura variable to expected task contract input",
        "\t\tfaturaInput: {",
        "\t\t\tvalidacao: $data.fatura.validacao ? {",
        "\t\t\t\tpersistenceId_string: $data.fatura.validacao.persistenceId_string !== undefined ? $data.fatura.validacao.persistenceId_string : null,",
        "\t\t\t\tcomercial: $data.fatura.validacao.comercial !== undefined ? $data.fatura.validacao.comercial : null",
        "\t\t\t} : null",
        "\t\t}",
        "\t}",
        "}"
      ],
