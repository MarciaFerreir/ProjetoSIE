    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.proposta && $data.proposta_cliente ){",
        "\t//attach lazy references variables to parent variables",
        "\t$data.proposta.cliente = $data.proposta_cliente;",
        "\treturn {",
        "\t\t//map proposta variable to expected task contract input",
        "\t\tpropostaInput: {",
        "\t\t\trequesitos: $data.proposta.requesitos.map( it => ({",
        "\t\t\t\tpersistenceId_string: it.persistenceId_string !== undefined ? it.persistenceId_string : null,",
        "\t\t\t\tfeito: it.feito !== undefined ? it.feito : null",
        "\t\t\t}))",
        "\t\t}",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
