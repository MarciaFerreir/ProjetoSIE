        "\t$data.proposta.cliente = $data.proposta_cliente;",
        "\treturn {",
        "\t\t//map proposta variable to expected task contract input",
        "\t\tpropostaInput: {",
        "\t\t\tvalidacao: $data.proposta.validacao ? {",
        "\t\t\t\tcliente: $data.proposta.validacao.cliente !== undefined ? $data.proposta.validacao.cliente : null",
